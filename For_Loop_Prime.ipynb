   "source": [
    "n = int(input(\"Please enter positive number : \"))\n",
    "\n",
    "prime_number = []\n",
    "\n",
    "for i in range(2,n):\n",
    "    count = 0\n",
    "    for j in range(1,n):\n",
    "        if i % j == 0:\n",
    "            count += 1\n",
    "    if count < 3:\n",
    "        prime_number.append(i)\n",
    "                \n",
    "print(prime_number)"
