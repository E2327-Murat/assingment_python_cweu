   "source": [
    "leap = int(input(\"Please enter a year: \"))\n",
    "\n",
    "if leap % 400 == 0:\n",
    "    print(f\"{leap} is a leap year\")\n",
    "\n",
    "elif leap % 4 == 0 and leap % 100 != 0:\n",
    "    print(f\"{leap} is a leap year\")\n",
    "\n",
    "else:\n",
    "    print(f\"{leap} is not a leap year\")"
   ]
  }
